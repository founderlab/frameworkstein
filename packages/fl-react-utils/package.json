{
  "name": "fl-react-utils",
  "version": "10.0.10",
  "description": "Boilerplate and helper functions for react/redux that can be shared amongst FounderLab apps",
  "main": "lib/index.js",
  "author": {
    "name": "Gwilym Humphreys",
    "url": "https://github.com/gwilymhumphreys"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/founderlab/frameworkstein/tree/master/packages/fl-react-utils"
  },
  "scripts": {
    "prepare": "babel --root-mode upward ./src --out-dir ./lib --delete-dir-on-start",
    "build": "babel --root-mode upward ./src --out-dir ./lib --delete-dir-on-start",
    "watch": "babel --root-mode upward ./src --out-dir ./lib --delete-dir-on-start --watch",
    "test": "eval $(cat test/.env) mocha test/**/*.tests.js",
    "cover": "eval $(cat test/.env) istanbul cover _mocha -- test/**/*.tests.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0"
  },
  "dependencies": {
    "classnames": "^2.0.0",
    "inflection": "^1.0.0",
    "lodash": "^4.0.0",
    "moment-timezone": "^0.5.26",
    "prop-types": "*",
    "qs": "^6.0.0",
    "react": ">=16.0.0",
    "react-datetime": "^3.0.0",
    "react-dom": ">=16.0.0",
    "react-dropzone-s3-uploader": "^1.0.0",
    "react-markdown": "*",
    "react-redux": "^7.0.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-select": "^3.0.0",
    "react-sidebar": "^2.0.0",
    "reactstrap": "^8.0.0",
    "redux": "^4.0.0",
    "redux-form": "^8.0.0",
    "warning": "^2.0.0"
  },
  "gitHead": "e1e1a50162bd4b054f52769c447e5abe76af1383"
}
