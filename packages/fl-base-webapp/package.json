  "scripts": {
    "start": "node index.js",
    "start-dev": "eval $(cat .env) nodemon -w server --inspect index.js",
    "start-dev-all": "eval $(cat .env) nodemon -w server -w shared index.js",
    "start-staging": "eval $(cat .env) NODE_ENV=staging nodemon index.js",
    "client-watch": "eval $(cat .env) node webpack/webpack-dev-server.js",
    "dev": "npm run start-dev & npm run client-watch",
    "lint": "eslint server shared",
    "build": "NODE_ENV=production rm -rf public/dist && webpack --verbose --progress --profile --colors --display-error-details --config webpack/prod.config.js",
    "deploy-staging": "git checkout staging && git pull && git merge master --no-edit --strategy-option theirs && npm run build && git add . && git commit -am 'build' && git push && eb deploy",
    "quick-deploy-staging": "git checkout staging && git pull && git merge master --no-edit --strategy-option theirs && git push && eb deploy",
    "deploy-production": "git checkout production && git pull && git merge master --no-edit --strategy-option theirs && npm run build && git add . && git commit -am 'build' && git push && eb deploy",
    "quick-deploy-production": "git checkout production && git pull && git merge master --no-edit --strategy-option theirs && git push && eb deploy",
    "scaffold": "eval $(cat .env) node ./scaffold/run.js",
    "test": "eval $(cat __tests__/.env) jest --runInBand"
  },
