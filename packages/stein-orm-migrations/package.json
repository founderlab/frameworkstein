{
  "name": "stein-orm-migrations",
  "version": "6.14.8",
  "description": "Automatic migrations for stein-orm",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "rm -rf ./lib && babel ./src --ignore '/node_modules/' --out-dir ./lib",
    "build": "rm -rf ./lib && babel ./src --ignore '/node_modules/' --out-dir ./lib",
    "watch": "rm -rf ./lib && babel ./src --ignore '/node_modules/' --watch --out-dir ./lib",
    "test": "eval $(cat __tests__/.env) jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/founderlab/stein-orm-migrations"
  },
  "author": "Danny Sullivan <dbsullivan23@gmail.com>",
  "license": "MIT",
  "keywords": [
    "migrations"
  ],
  "bugs": {
    "url": "https://github.com/founderlab/frameworkstein/issues"
  },
  "homepage": "https://github.com/founderlab/packages/stein-orm-migrations#readme",
  "engines": {
    "node": "8.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "jest": "^22.0.0",
    "stein-orm": "^6.14.8"
  },
  "dependencies": {
    "fl-server-utils": "^6.7.6",
    "stein-orm": "^6.14.8",
    "stein-orm-sql": "^6.14.8"
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "testPathIgnorePatterns": [
      "node_modules/!(stein-orm)/*"
    ]
  }
}
