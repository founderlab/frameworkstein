{
  "name": "stein-fetch",
  "version": "8.0.13",
  "description": "Fetch wrapper that allows requests to be built via chaining functions",
  "main": "lib/index.js",
  "author": {
    "name": "Gwilym Humphreys",
    "url": "https://github.com/gwilymhumphreys"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/founderlab/frameworkstein/tree/master/packages/stein-fetch"
  },
  "scripts": {
    "prepublish": "babel --root-mode upward ./src --out-dir ./lib --delete-dir-on-start",
    "build": "babel --root-mode upward ./src --out-dir ./lib --delete-dir-on-start",
    "watch": "babel --root-mode upward ./src --out-dir ./lib --delete-dir-on-start --watch",
    "test": "eval $(cat __tests__/.env) jest --runInBand"
  },
  "dependencies": {
    "cross-fetch": "^3.0.0",
    "lodash": "^4.0.0",
    "qs": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-jest": "^24.0.0",
    "jest": "^27.0.0"
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
